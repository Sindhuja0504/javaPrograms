package org;

public class Mobile {
	void iphone()
	{
		System.out.println("my iphone ");
	}
	void  samsung()
	{
		System.out.println("my samsung phone");
	}
	void oppo()
	{
		System.out.println("my oppo phone");
	}
	

	public static void main(String[] args) {
		
		Mobile obj = new Mobile();
		Mobile phone = new Mobile();
		obj.iphone();
		obj.oppo();
		phone.samsung();
		

	}

}
