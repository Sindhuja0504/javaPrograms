package org;

class Animalss {
	String name;
	int age;

	void makesound() {
		System.out.println("Animals makes sound");
	}
}

class dog1 extends Animalss {
	String breed;

	@Override
	void makesound() {
		
	}

	void fetch() {
		System.out.println("Dog1 is fetching");
	}

	}

public class InheritanceType1 {

	public static void main(String[] args) {
		hello d1 = new hello();
		d1.breed = "puppy";
		System.out.println(d1.breed);

	}

}
