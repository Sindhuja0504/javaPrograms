package org;

public class ModulusOperator {
// comparison operator
	public static void main(String[] args) {
		// create a num 
		//find out the number 3 is divisible are not
		//
		int num = 15;
		//System.out.println(num%3);
		if (15%3==0 && 15%5==0) {
			System.out.println("true");
			
		} else {
			System.out.println("false");

		}
		if ( 15%5==0) {
			System.out.println("true");
			
		} else {
			System.out.println("false");

		}

	}

}
