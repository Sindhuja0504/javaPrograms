package org;

public class Garden {
	
	int apple_price = 20;
	int apple_count = 5;
	
	void total_money() {
		int total = apple_price *apple_count;
		System.out.println("the total ap[ple price is : " +total);
	}

	public static void main(String[] args) {
		
		Garden num = new Garden();
		num.total_money();
 
	}

}
