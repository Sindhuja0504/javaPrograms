package org;

import java.util.Scanner;

public class ForLoop {

	public static void main(String[] args) {
	//1.
		Scanner obj = new Scanner(System.in);
		System.out.println("enter i value");
	    int i= obj.nextInt();
	//2.
		int a= 5;
		int b= 10;
		for(a=5;a<=b; a=a+1) {
			System.out.println(a);
}
	}
}