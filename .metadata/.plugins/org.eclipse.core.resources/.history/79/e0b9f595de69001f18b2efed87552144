package seleniumBasic;

public class Parent {
	
	void gear(){
		System.out.println("this is parent class : gear");
	}
		
		public void applybreak() {
			System.out.println("abs break");
			
		}
	
	
	public static void main(String[] args) {
		
		Parent demo = new Parent();
		demo.applybreak();
		demo.gear();
//		demo.horn();
		
	}

}
