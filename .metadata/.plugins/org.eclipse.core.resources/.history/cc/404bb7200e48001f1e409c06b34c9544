package org;

import java.util.Scanner;

public class DemoSacnner {
	
	 static int a=1;
	 int b=5;
	
	public void hello() {
		   int a=2;              // local variable
		System.out.println("This is local variable :"+a);
	}

	public static void main(String[] args) {
		DemoSacnner bs =new DemoSacnner();
		
		bs.hello();
		
		System.out.println(bs.a);
		System.out.println(bs.b);
		
